<Page x:Class="WpfApp1.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1">

    <Grid Margin="-43,0,0,-0.4" HorizontalAlignment="Left" Width="568">
        <TextBlock x:Name="main_Title" HorizontalAlignment="Center" Margin="195,45,148,346" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="22" TextAlignment="Center" Width="328"><Run Text="TextBox"/><LineBreak/><Run/></TextBlock>
        <Button x:Name="feeling_tracker" Content="Feeling Tracker" HorizontalAlignment="Left" Margin="392,109,0,0" VerticalAlignment="Top" Width="100" Height="40" Click="Feeling_tracker_Click"/>
        <Button x:Name="exercise_tracker" Content="Exercise Tracker" HorizontalAlignment="Left" Margin="392,182,0,0" VerticalAlignment="Top" Width="100" Height="40" Click="Exercise_tracker_Click" RenderTransformOrigin="0.511,-0.762"/>
        <Button x:Name="Sleep_Tracker" Content="Sleep Tracker" HorizontalAlignment="Left" Margin="392,263,0,0" VerticalAlignment="Top" Width="100" Height="40" Click="Sleep_Tracker_Click"/>
        <Button x:Name="Food_Tracker" Content="Food Tracker" HorizontalAlignment="Left" Margin="392,338,0,0" VerticalAlignment="Top" Width="100" Height="40" Click="Food_Tracker_Click"/>
        <DataGrid x:Name="Overall_data" HorizontalAlignment="Left" Height="269" Margin="77,109,0,0" VerticalAlignment="Top" Width="283" SelectionChanged="DataGrid_SelectionChanged" RenderTransformOrigin="0.69,0.294"/>
    </Grid>
</Page>
